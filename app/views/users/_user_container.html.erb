<ul>
  <% users.each do |user| %>
    <li>
    <%= link_to user.username, user_path(user), class: "red"%>
    <%= form_for(relationship) do |f| %>
      <%= f.hidden_field :user_id, :value => user.id %>
      <%= f.submit "follow", :class => 'button-blue-outline mb1' %>
    <% end %>
    </li>
  <% end %>
</ul>
